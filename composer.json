{
    "name": "xenon/sslcommerz",
    "description": "Laravel library for SSLCommerz payment gateway",
    "keywords": [
        "laravel",
        "payment",
        "gateway",
        "sslcommerz",
        "gateway"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahabubul Hasan",
            "email": "codehasan@gmail.com"
        }
    ],
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Xenon\\SslCommerz\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": ">=5.3.0",
        "guzzlehttp/guzzle": "^6.3|>=7.3.0",
        "touhidurabir/laravel-stub-generator": "^1.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xenon\\SslCommerz\\SslCommerzServiceProvider"
            ]
        }
    }
}
